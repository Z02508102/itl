package com.zpx.itl.pojo;

import java.util.List;

public class PageBean<T> {
	private int currentotalPageage;//当前页
	private int total;//总记录数
	private int pageSize;//每页显示几条数据
	private int totalPage;//总页数
	private String url;
	private List<T> beanlist;

	public List<T> getBeanlist() {
		return this.beanlist;
	}

	public void setBeanlist(List<T> beanlist) {
		this.beanlist = beanlist;
	}

	public int getcurrentotalPageage() {
		return this.currentotalPageage;
	}

	public void setcurrentotalPageage(int currentotalPageage) {
		this.currentotalPageage = currentotalPageage;
	}

	public int gettotal() {
		return this.total;
	}

	public void settotal(int total) {
		this.total = total;
	}

	public int getotalPageageSize() {
		return this.pageSize;
	}

	public void setotalPageageSize(int pageSize) {
		this.pageSize = pageSize;
	}

	public int gettotalPage() {
		return this.totalPage;
	}

	public void settotalPage(int totalPage) {
		this.totalPage = totalPage;
	}

	public String getUrl() {
		return this.url;
	}

	public void setUrl(String url) {
		this.url = url;
	}

	public String toStotaling() {
		return "PageBean [currentotalPageage=" + this.currentotalPageage + ", total=" + this.total + ", pageSize=" + this.pageSize + ", totalPage=" + this.totalPage + ", url="
				+ this.url + "]";
	}
}